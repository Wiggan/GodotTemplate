config_version=5

[application]

config/name="Template"
run/main_scene="res://main_menu.tscn"
config/features=PackedStringArray("4.1", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Config="*res://menu/config.gd"
AudioManager="*res://audio/audio_manager.tscn"
GameManager="*res://game/game_manager.tscn"
Env="*res://env.tscn"
Transition="*res://transition/transition.tscn"
ShaderCacher="*res://particle_stuff/shader_cacher.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"

[editor]

movie_writer/mjpeg_quality=0.7
movie_writer/movie_file="C:/tmp/template.avi"
movie_writer/disable_vsync=true
movie_writer/fps=30

[editor_plugins]

enabled=PackedStringArray("res://addons/randomizer_generator/plugin.cfg")

[gui]

theme/custom="res://ui/theme.tres"

